{
  "name": "buzz",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant debug",
    "android:clean": "cd android && gradlew clean",
    "ios": "react-native run-ios --scheme buzz.development",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios:install": "cd ios && pod install",
    "ios:update": "cd ios && pod update && pod install",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --verbose --watch",
    "code:check": "tsc --project tsconfig.lint.json && eslint . --ext .js,.jsx,.ts,.tsx",
    "code:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "images:update": "ts-node scripts/images.ts"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/bottom-tabs": "^5.1.1",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "await-timeout": "^1.1.1",
    "buffer": "^5.6.0",
    "formik": "^2.1.4",
    "i18next": "^19.3.3",
    "jwt-decode": "^2.2.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-auth0": "^2.3.0",
    "react-native-config": "^0.12.0",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-hyperlink": "^0.0.19",
    "react-native-keychain": "^6.0.0",
    "react-native-localize": "^1.3.4",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "typesafe-actions": "^5.1.0",
    "whatwg-url": "^8.0.0",
    "yup": "^0.28.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/await-timeout": "^0.3.1",
    "@types/jest": "^24.0.24",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^13.9.0",
    "@types/react-native": "^0.60.25",
    "@types/react-native-auth0": "^2.0.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "16.9.1",
    "@types/redux-logger": "^3.0.7",
    "@types/whatwg-url": "^6.4.0",
    "@types/yup": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
