{
  "name": "buzz",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant debug",
    "android:assembleRelease": "cd android && gradlew assembleRelease",
    "android:bundleRelease": "cd android && gradlew bundleRelease",
    "android:runRelease": "react-native run-android --variant=release",
    "android:clean": "cd android && gradlew clean",
    "ios": "react-native run-ios",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios:install": "cd ios && pod install",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --verbose --watch",
    "code:check": "tsc --project tsconfig.json && eslint . --ext .js,.jsx,.ts,.tsx",
    "code:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "rimraf node_modules yarn.lock && yarn cache clean --force",
    "reset": "react-native start --reset-cache"
  },
  "dependencies": {
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-config": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
