{
  "name": "buzz",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant debug",
    "android:clean": "cd android && gradlew clean",
    "ios": "react-native run-ios --scheme buzz.development",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios:install": "cd ios && pod install",
    "ios:update": "cd ios && pod update && pod install",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --verbose --watch",
    "code:check": "tsc --project tsconfig.lint.json && eslint . --ext .js,.jsx,.ts,.tsx",
    "code:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "images:update": "ts-node scripts/images.ts",
    "upgrade": "react-native upgrade",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "reset": "react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.3.4",
    "@react-navigation/native": "^5.2.4",
    "@react-navigation/stack": "^5.2.19",
    "await-timeout": "^1.1.1",
    "buffer": "^5.6.0",
    "formik": "^2.1.4",
    "i18next": "^19.4.4",
    "jwt-decode": "^2.2.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-auth0": "^2.3.1",
    "react-native-config": "^1.0.0",
    "react-native-error-boundary": "^1.1.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-hyperlink": "^0.0.19",
    "react-native-keychain": "^6.0.0",
    "react-native-localize": "^1.4.0",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-switch-selector": "^2.0.4",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "typesafe-actions": "^5.1.0",
    "whatwg-url": "^8.0.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/await-timeout": "^0.3.1",
    "@types/jest": "^25.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^13.13.5",
    "@types/react-native": "^0.62.7",
    "@types/react-native-auth0": "^2.0.1",
    "@types/react-redux": "^7.1.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.7",
    "@types/whatwg-url": "^6.4.0",
    "@types/yup": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-flipper": "^0.39.0",
    "react-test-renderer": "16.11.0",
    "redux-flipper": "^1.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
